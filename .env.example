# Django Secret Key
# Generate a new secret key for production: https://djecrety.ir/
SECRET_KEY=your-secret-key-here

# Agora Credentials
# Get these from: https://console.agora.io/
# 1. Sign up for a free Agora account
# 2. Create a new project
# 3. Enable "App Certificate" for the project
# 4. Copy your App ID and Certificate below
AGORA_APP_ID=your_agora_app_id_here
AGORA_APP_CERTIFICATE=your_agora_app_certificate_here
AGORA_CUSTOMER_ID=
AGORA_CUSTOMER_CERTIFICATE=

# AWS S3 Credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_STORAGE_BUCKET_NAME=
AWS_S3_REGION_NAME=

